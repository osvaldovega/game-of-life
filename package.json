{
  "name": "game-of-life",
  "version": "0.1.0",
  "description": "Game of live of John Conway's, in Nextjs",
  "keywords": [
    "nextjs",
    "typescript",
    "react"
  ],
  "private": true,
  "scripts": {
    "build": "next build && next export -o dist",
    "check-types": "tsc",
    "dev": "next dev",
    "lint:fix": "eslint . --fix",
    "lint:report": "eslint --no-error-on-unmatched-pattern -f json -o report.json ./src",
    "lint": "next lint",
    "start": "next start"
  },
  "dependencies": {
    "@material-ui/core": "4.12.3",
    "@material-ui/icons": "4.11.2",
    "@material-ui/styles": "4.11.4",
    "next": "11.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-components": "5.3.0"
  },
  "devDependencies": {
    "@types/material-ui": "0.21.9",
    "@types/node": "16.6.1",
    "@types/react": "17.0.18",
    "@types/react-dom": "17.0.9",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.0",
    "eslint-config-prettier": "8.3.0",
    "immer": "9.0.5",
    "prettier": "2.3.2",
    "sass": "1.37.5",
    "typescript": "4.3.5"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:osvaldovega/game-of-life.git"
  },
  "author": "Osvaldo Vega",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.20.0",
    "yarn": "^1.22.0"
  }
}
